<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dai.dao.RegisterMapper">

    <select id="getIds" resultType="int">
        SELECT count(user.id)
        FROM user
    </select>

    <select id="findAllAccount" resultType="String">
        SELECT account FROM user
    </select>

    <insert id="insertData" useGeneratedKeys="true">
        INSERT INTO user (account, password, nick, age,sex,number,head,email,introduction)
        VALUES (#{account}, #{password}, #{nick}, #{age}, #{sex}, #{number}, #{head}, #{email},#{introduction})
    </insert>

</mapper>